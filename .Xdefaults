*.color3:       #cdcd00
*.color11:      #ffff00

! blue
*.color4:       #6495ed
*.color12:      #5c5cff

! magenta
*.color5:       #cd00cd
*.color13:      #ff00ff

! cyan
*.color6:       #00cdcd
