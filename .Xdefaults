URxvt.font: xft:Hack:size=13:antialias=true
URxvt.scrollBar: false
URxvt.cursorBlink: true
URxvt.saveLine: 10000
URxvt.skipScroll: false
URxvt.iso14755: false
URxvt.perl-ext-common: readline,searchable-scrollback,selection,matcher,font-size
